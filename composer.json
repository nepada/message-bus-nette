{
    "name": "nepada/message-bus-nette",
    "type": "library",
    "license": "BSD-3-Clause",
    "description": "Integration of nepada/message-bus into Nette Framework.",
    "keywords": ["nette", "messenger", "message bus"],
    "support": {
        "email": "petr@pada.cz",
        "issues": "https://github.com/nepada/message-bus-nette/issues"
    },
    "authors": [
        {
            "name": "Petr MorÃ¡vek",
            "email": "petr@pada.cz"
        }
    ],
    "require": {
        "php": ">=7.4.0 <8.3",
        "symfony/messenger": "^5.0@dev || ^6.0@dev",
        "nepada/message-bus": "^2.0@dev",
        "nette/di": "^3.0.5@dev",
        "nette/utils": "^3.1.3@dev || ^4.0@dev",
        "nette/schema": "^1.0.2@dev",
        "psr/container": "^1.1 || ^2.0"
    },
    "require-dev": {
        "nette/tester": "2.4.3",
        "phpstan/phpstan": "1.10.14",
        "phpstan/phpstan-strict-rules": "1.5.1",
        "phpstan/phpstan-nette": "1.2.9",
        "nepada/phpstan-nette-tester": "1.0.0",
        "spaze/phpstan-disallowed-calls": "2.14.2",
        "shipmonk/phpstan-rules": "2.5.1",
        "php-parallel-lint/php-parallel-lint": "1.3.2",
        "nepada/coding-standard": "7.9.0",
        "nepada/message-bus-doctrine": "^2.1@dev",
        "nette/bootstrap": ">=3.1@dev",
        "nette/robot-loader": ">=3.3.1@dev",
        "doctrine/cache": "^1.11",
        "doctrine/dbal": ">=2.13.3",
        "doctrine/annotations": ">=1.8"
    },
    "autoload": {
        "psr-4": {
            "Nepada\\MessageBusNette\\": "src/MessageBusNette/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "NepadaTests\\": "tests/"
        }
    },
    "config": {
        "platform-check": false,
        "allow-plugins": {
            "dealerdirect/phpcodesniffer-composer-installer": false
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "2.2-dev"
        }
    }
}
