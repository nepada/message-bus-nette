{
    "name": "nepada/message-bus-nette",
    "type": "library",
    "license": "BSD-3-Clause",
    "description": "Integration of nepada/message-bus into Nette Framework.",
    "keywords": ["nette", "messenger", "message bus"],
    "support": {
        "email": "petr@pada.cz",
        "issues": "https://github.com/nepada/message-bus-nette/issues"
    },
    "authors": [
        {
            "name": "Petr MorÃ¡vek",
            "email": "petr@pada.cz"
        }
    ],
    "require": {
        "php": ">=7.4.0",
        "symfony/messenger": "^5.0@dev",
        "nepada/message-bus": "^2.0@dev",
        "nette/di": "^3.0.5@dev",
        "nette/utils": "^3.1.3@dev",
        "nette/schema": "^1.0.2@dev",
        "psr/container": "^1.0@dev"
    },
    "require-dev": {
        "nette/tester": "2.4.0",
        "phpstan/phpstan": "0.12.91",
        "phpstan/phpstan-strict-rules": "0.12.10",
        "pepakriz/phpstan-exception-rules": "0.11.7",
        "phpstan/phpstan-nette": "0.12.19",
        "nepada/phpstan-nette-tester": "0.3.1",
        "spaze/phpstan-disallowed-calls": "1.4.0",
        "php-parallel-lint/php-parallel-lint": "1.3.0",
        "nepada/coding-standard": "7.4.0",
        "nette/bootstrap": "^3.0@dev",
        "nette/robot-loader": "^3.2.4@dev",
        "nepada/message-bus-doctrine": "^2.0@dev",
        "doctrine/cache": "^1.11"
    },
    "autoload": {
        "psr-4": {
            "Nepada\\MessageBusNette\\": "src/MessageBusNette/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "NepadaTests\\": "tests/"
        }
    },
    "config": {
        "platform-check": false
    },
    "extra": {
        "branch-alias": {
            "dev-master": "2.1-dev"
        }
    }
}
