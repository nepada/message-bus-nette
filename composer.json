{
    "name": "nepada/message-bus-nette",
    "type": "library",
    "license": "BSD-3-Clause",
    "description": "Integration of nepada/message-bus into Nette Framework.",
    "keywords": ["nette", "messenger", "message bus"],
    "support": {
        "email": "petr@pada.cz",
        "issues": "https://github.com/nepada/message-bus-nette/issues"
    },
    "authors": [
        {
            "name": "Petr MorÃ¡vek",
            "email": "petr@pada.cz"
        }
    ],
    "require": {
        "php": ">=8.2.0 <8.5",
        "symfony/messenger": "^5.0@dev || ^6.0@dev || ^7.0@dev",
        "nepada/message-bus": "^2.2@dev || ^3.0@dev",
        "nette/di": "^3.0.9@dev",
        "nette/utils": "^3.2.5@dev || ^4.0@dev",
        "nette/schema": "^1.2.2@dev",
        "psr/container": "^1.1 || ^2.0"
    },
    "require-dev": {
        "nette/tester": "2.5.4",
        "phpstan/phpstan": "1.12.7",
        "phpstan/phpstan-strict-rules": "1.6.1",
        "phpstan/phpstan-nette": "1.3.8",
        "nepada/phpstan-nette-tester": "1.2.1",
        "spaze/phpstan-disallowed-calls": "3.5.1",
        "shipmonk/phpstan-rules": "3.2.1",
        "php-parallel-lint/php-parallel-lint": "1.4.0",
        "nepada/coding-standard": "7.14.1",
        "nepada/message-bus-doctrine": "^2.1@dev",
        "nette/bootstrap": ">=3.1@dev",
        "nette/robot-loader": ">=3.3.1@dev",
        "nette/neon": ">=3.3.4@dev",
        "doctrine/orm": ">=2.13",
        "doctrine/dbal": ">=2.13.3",
        "symfony/cache": "^6.3.12 || ^6.4.3 || ^7.0.3 || >=7.1",
        "symfony/cache-contracts": ">=3.4.2",
        "composer-runtime-api": "^2.0",
        "composer/semver": "3.4.3"
    },
    "autoload": {
        "psr-4": {
            "Nepada\\MessageBusNette\\": "src/MessageBusNette/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "NepadaTests\\": "tests/"
        }
    },
    "config": {
        "platform-check": false,
        "allow-plugins": {
            "dealerdirect/phpcodesniffer-composer-installer": false
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "2.4-dev"
        }
    }
}
